package edu.fju.exam;

public class Niner {

	public static void main(String[] args) {
		for ( int k = 0 ; k < 9 ; k += 3 )

		{

		  for ( int j = 2 ; j <=5 ; j++ )

		  {

		    for ( int i = 1 ; i <= 3 ; i++ )

		    {

		        System.out.printf ( "%d * %d = %d \t" , i + k , j , j * ( i + k ) );

		    }

		    System.out.println();

		  }

		  System.out.println();

		}
		
	}
}


